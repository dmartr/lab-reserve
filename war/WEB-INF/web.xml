<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Es_upm_dit_isst_labreserve</servlet-name>
		<servlet-class>es.upm.dit.isst.labreserve.Es_upm_dit_isst_labreserveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Es_upm_dit_isst_labreserve</servlet-name>
		<url-pattern>/es_upm_dit_isst_labreserve</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>ResourceApplication.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>Main</servlet-name>
		<servlet-class>es.upm.dit.isst.labreserve.MainServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Main</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CreateResource</servlet-name>
		<servlet-class>es.upm.dit.isst.labreserve.CreateResourceServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RemoveResource</servlet-name>
		<servlet-class>es.upm.dit.isst.labreserve.RemoveResourceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemoveResource</servlet-name>
		<url-pattern>/done</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateResource</servlet-name>
		<url-pattern>/new</url-pattern>
	</servlet-mapping>
</web-app>
